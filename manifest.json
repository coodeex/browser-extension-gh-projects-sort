{
  "manifest_version": 3,
  "name": "Custom Sort Extension",
  "version": "1.0",
  "description": "Sorts items based on updatedAt.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentScript.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}